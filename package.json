{
  "name": "sarahlim.github.io",
  "version": "0.0.1",
  "description": "personal website for Sarah Lim",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve & npm run watch",
    "serve": "harp server _harp --port 9001",
    "watch": "browser-sync start --proxy 'localhost:9001' --files '_harp/**/*.jade, _harp/**/*.scss, _harp/**/*.json, _harp/**/*.js, _harp/**/*.md'",
    "build": "harp compile _harp ./",
    "lint": "eslint . || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarahlim/sarahlim.github.io.git"
  },
  "author": "Sarah Lim <sarah@sarahlim.com> (http://sarahlim.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sarahlim/sarahlim.github.io/issues"
  },
  "homepage": "https://sarahlim.com",
  "dependencies": {
    "harp": "^0.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.5",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "postcss": "^5.2.6",
    "stylelint": "^7.7.0",
    "stylelint-config-guidelines": "^1.1.5"
  }
}
